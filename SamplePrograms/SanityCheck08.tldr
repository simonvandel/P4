actor main := {
  receive arguments:args := {

    let x:int := 5;
    let y:int := x;

    let z:int := plus(x, y);

    if(z = 10){
      print("Expected");
    }
    else {
      print("Not expected");
    }
  };
};

let plus(a,b) : int -> int -> int := {
  a + b
  };



// z is expected to be 10
